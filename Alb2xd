                print(a, b, c, d)
        
                local smallest = a  -- initialize smallest variable to the first number
        
                if b < smallest then
                    smallest = b
                end
        
                if c < smallest then
                    smallest = c
                end
        
                if d < smallest then
                    smallest = d
                end
        
                checkValue = smallest
        
