 local Tab = Window:CreateTab("Personalizado", 4483362458) -- Title, Image
 local Slider = Tab:CreateSlider({
    Name = "Speed",
    Range = {25, 50000},
    Increment = 1,
    Suffix = "Velicidad",
    CurrentValue = 17,
    Flag = "Velocidad", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        getgenv().settings = {
            ['FLYING'] = {
                ['ON'] = true;
                ['SPEED'] = Value;
            };
        }
        local s = getgenv().settings['FLYING']['SPEED']
        while getgenv().settings['FLYING']['ON'] == true do
            game:GetService("RunService").Heartbeat:wait();
            getrenv()._G.FlightSpeed = getgenv().settings['FLYING']['SPEED'];
            if s ~= getgenv().settings['FLYING']['SPEED'] then
                break;
            end;
        end;
    -- The function that takes place when the slider changes
    -- The variable (Value) is a number which correlates to the value the slider is currently at
    end,
 })




 --requerimiento para ir a la tierra
 local Slider = Tab:CreateSlider({
    Name = "RequerimientoTierra",
    Range = {50000,800000},
    Increment = 1,
    Suffix = "AtaquesManuales",
    CurrentValue = 50000,
    Flag = "StatsManuales", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        local player = game.Players.LocalPlayer
		local statsFolder = game.Workspace.Living[player.Name].Stats
		local strength = statsFolder.Strength
		local speed = statsFolder.Speed
		local energy = statsFolder.Energy
		local rebirth = statsFolder.Rebirth
		local defense = statsFolder.Defense
		local ki=statsFolder.Ki
spawn(function ()
while punchsea do
    wait()
    if strength.Value<=Value then
    game:GetService("ReplicatedStorage").Package.Events.p:FireServer(unpack(args))
end 
end
end)

spawn(function ()
    while energyaweda do
        wait()
        if energy.Value<=Value then
            game:GetService("ReplicatedStorage").Package.Events.kb:FireServer(unpack(args))
    end 
    end
    end)
    spawn(function ()
        while defenseaea do
            wait()
            if defense.Value<=Value then
                game:GetService("ReplicatedStorage").Package.Events.def:InvokeServer(unpack(args))
        end 
        end
        end)
        spawn(function ()
            while speedeeed do
                wait()
                if speed.Value<=Value then
                    game:GetService("ReplicatedStorage"):WaitForChild("Package"):WaitForChild("Events"):WaitForChild("ch"):InvokeServer(unpack(args))
                    game:GetService("ReplicatedStorage"):WaitForChild("Package"):WaitForChild("Events"):WaitForChild("of"):FireServer()
            end 
            end
            end)
    end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "Requerimiento Si o No",
    CurrentValue = false,
    Flag = "MinimumStatsManuales", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(acas)

       _G.statsrequerimientose=false
    punchsea=false
    energyaweda=false
    defenseaea=false
    speedeeed=false

    
if acas then
    _G.statsrequerimientose=true
    punchsea=true
    energyaweda=true
    defenseaea=true
    speedeeed=true

end

    -- The function that takes place when the toggle is pressed
    -- The variable (Value) is a boolean on whether the toggle is true or false
    end,
 })
