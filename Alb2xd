 local Tab = Window:CreateTab("TP", 4483362458) -- Title, Image
--tp a Bills Planet
--con un click ejecutas el codigo
local Button = Tab:CreateButton({
    Name = "TP Planeta De Bills",
    Callback = function()
        local A_1 = "Vills Planet"
        local Event = game:GetService("ReplicatedStorage").Package.Events.TP
        Event:InvokeServer(A_1)  -- The function that takes place when the button is pressed
    end,
 })

